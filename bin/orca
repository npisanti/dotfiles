#!/bin/bash

if [ -z "$1" ]; then
    chrt --rr 79  /home/$USER/apps/Orca-c/build/release/orca --strict-timing --undo-limit 20 --osc-port 4444 
else
    #FILEPATH=$(realpath "$1")
    chrt --rr 79  /home/$USER/apps/Orca-c/build/release/orca --strict-timing --undo-limit 20  --osc-port 4444 $1
fi

exit
